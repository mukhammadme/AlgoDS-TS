{
  "name": "algods-ts",
  "version": "1.0.0",
  "description": "Algorithm and Data Structures in TypeScript",
  "main": "index.ts",
  "scripts": {
    "cleanup": "npx rimraf dist",
    "build": "npm run cleanup && tsc -p .",
    "build:dev": "tsc -w -p .",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register ./src/index.ts",
    "test": "jest",
    "test:dev": "jest --watch --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mukhammadme/AlgoDS-TS.git"
  },
  "keywords": [
    "algorithm",
    "algorithms",
    "data-structures",
    "ds",
    "typescript",
    "ts"
  ],
  "author": "Mukhammad Pardaboev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mukhammadme/AlgoDS-TS/issues"
  },
  "homepage": "https://github.com/mukhammadme/AlgoDS-TS#readme",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
